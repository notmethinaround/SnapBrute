{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "hmmm",
            "request": "launch",
            "name": "Debug HMMM",
            "program": "${file}",
            "isBinary": true
        },
        {
            "type": "hmmm",
            "request": "launch",
            "name": "Debug HMMM (Binary)",
            "program": "${file}",
            "isBinary": true
        },
        {
            "name": "Debug HMMM (Binary)",
            "type": "hmmm",
            "request": "launch",
            "program": "${file}",
            "isBinary": true
        }
    ]
}